<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Test</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.min.css"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute"
              crossorigin="anonymous"
              integrity="sha256-eSi1q2PG6J7g7ib17yAaWMcrr5GrtohYChqibrV7PBE="/>
    </environment>
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">CCIA Web App</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" href="https://ccia.ucdavis.edu/">CCIA Homepage</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <nav class="navbar navbar-expand-md bg-dark navbar-dark">
    <a href="/" class="navbar-brand">Members</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbars">
        <span class="navbar-toggler-icon"></span>
    </button>
    <div class="navbar-collapse collapse navbars" id="navbar2">
        <ul class="navbar-nav">             
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Field Applications</a>
                <div class="dropdown-menu">
                    <a class="dropdown-item" asp-controller="Application" asp-action="Index">Application Home</a>
                    <div class="dropdown-divider"></div>
                    <a class="dropdown-item" asp-controller="Application" asp-action="Index">Grow Certified Seed</a>
                    <a class="dropdown-item" asp-controller="Application" asp-action="Index">Grow Certified Potatoes</a>
                    <a class="dropdown-item" asp-controller="Application" asp-action="Index">Heritage Grain QA</a>
                    <a class="dropdown-item" asp-controller="Application" asp-action="Index">Pre-Variety Germplasm</a>
                    <a class="dropdown-item" asp-controller="Application" asp-action="Index">Rice QA</a>
                    <a class="dropdown-item" asp-controller="Application" asp-action="Index">Hemp</a>                            
                    <div class="dropdown-divider"></div>
                    <a class="dropdown-item" asp-controller="Application" asp-action="Index">FIR Summary</a>
                    <a class="dropdown-item" asp-controller="Application" asp-action="Index">Application Renewals</a>
                </div>
            </li>
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" data-toggle="dropdown" asp-controller="Seeds" asp-action="Index" aria-haspopup="true" aria-expanded="false">Certify Seed Lots</a>
                <div class="dropdown-menu">
                     <a class="dropdown-item" asp-controller="Seeds" asp-action="Index">Seeds Home</a>
                     <a class="dropdown-item" asp-controller="Seeds" asp-action="Index">New Certified Seed Lot</a>
                     <a class="dropdown-item" asp-controller="Seeds" asp-action="Index">New QA/PVG Lot</a>
                </div>
            </li>
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" data-toggle="dropdown" asp-controller="Tag" asp-action="Index" aria-haspopup="true" aria-expanded="false">Tags</a>
                <div class="dropdown-menu">
                     <a class="dropdown-item" asp-controller="Tag" asp-action="Index">Tags Home</a>
                     <a class="dropdown-item" asp-controller="Tag" asp-action="Index">Request New Tag</a>
                     <a class="dropdown-item" asp-controller="Tag" asp-action="Index">Bulk Tag Request</a>
                     <a class="dropdown-item" asp-controller="Tag" asp-action="Index">OECD-Gray Tag/Not Finally Certified</a>
                </div>
            </li>
            <a class="nav-link" asp-area="" asp-controller="OECD" asp-action="Index">OECD</a>
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" data-toggle="dropdown" asp-controller="Blend" asp-action="Index" aria-haspopup="true" aria-expanded="false">Blends</a>
                <div class="dropdown-menu">
                     <a class="dropdown-item" asp-controller="Blend" asp-action="Index">Blends Home</a>
                     <a class="dropdown-item" asp-controller="Blend" asp-action="Index">New Lot Blend</a>
                     <a class="dropdown-item" asp-controller="Blend" asp-action="Index">New Varietal Blend</a>
                     <a class="dropdown-item" asp-controller="Blend" asp-action="Index">New InDirt Blend</a>
                </div>
            </li>
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" data-toggle="dropdown" asp-controller="BulkSalesCertificates" asp-action="Index" aria-haspopup="true" aria-expanded="false">Bulk Sales/Transfers</a>
                <div class="dropdown-menu">
                     <a class="dropdown-item" asp-controller="BulkSalesCertificates" asp-action="Index">New Bulks Sales Certificate</a>
                     <a class="dropdown-item" asp-controller="BulkSalesCertificates" asp-action="Index">Completed Bulk Sales Certificates</a>
                     <a class="dropdown-item" asp-controller="BulkSalesCertificates" asp-action="Index">New Seed Transfer Certificate</a>
                     <a class="dropdown-item" asp-controller="BulkSalesCertificates" asp-action="Index">Completed Seed Transfer Certificate</a>
                     <a class="dropdown-item" asp-controller="BulkSalesCertificates" asp-action="Index">My Customers</a>
                </div>
            </li>
             <a class="nav-link" asp-area="" asp-controller="TurfgrassCertificate" asp-action="Index">Turfgrass Harvest Certificates</a>
        </ul>
    </div>
</nav>

    </header>
    <div class="container">
        <partial name="_CookieConsentPartial" />
         @if (!string.IsNullOrWhiteSpace(TempData["ErrorMessage"] as string))
    {
        <div class="alert alert-danger">
            <button type="button" class="close" data-dismiss="alert">×</button>
            @TempData["ErrorMessage"]
        </div>
    }
    @if (!string.IsNullOrWhiteSpace(TempData["Message"] as string))
    {
        <div class="alert alert-info">
            <button type="button" class="close" data-dismiss="alert">×</button>
            @TempData["Message"]
        </div>
    }
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2019 
        </div>
    </footer>

    <environment include="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery"
                crossorigin="anonymous"
                integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=">
        </script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/js/bootstrap.bundle.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
                crossorigin="anonymous"
                integrity="sha256-E/V4cWE4qvAeO5MOhjtGtqDzPndRO1LBk8lJ/PR7CA4=">
        </script>
    </environment>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
