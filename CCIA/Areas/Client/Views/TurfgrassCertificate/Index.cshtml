@model CCIA.Models.IndexViewModels.TurgrassCertificateIndexViewModel


@{
    ViewData["Title"] = "Index";
}

@await Html.PartialAsync("_IndexHeaderPartial", @Model)

<div class="col-lg-12">
    <table id="table" class="table">
        <thead>  
            <th></th>          
        <th>AppId</th>
        <th>Type</th>
        <th>Cert Year</th>
        <th>Grower</th>
        <th>Field Name</th>
        <th>Acres</th>
        <th>County</th>
        <th>Crop</th>
        <th>Variety</th>
        <th>Class</th>        
        <th># Certificates</th>
        </thead>
        <tbody>
        
            @foreach (var app in Model.applications)
            {
            <tr>
                <td><a asp-action="Details" asp-controller="TurfgrassCertificate" asp-route-id="@app.Id">Details</a></td>
                <td>@app.Id</td>
                <td>@app.AppType</td>
                <td>@app.CertYear</td>
                <td>@app.ApplicantOrganization.OrgName</td>               
                <td>@app.FieldName</td>
                <td>@app.AcresApplied</td>
                <td>@app.County.Name</td>
                <td>@app.Variety.Crop.Name</td>
                <td>@app.Variety.Name</td>
                <td>@app.ClassProduced.ClassProducedTrans</td>
                <td>@app.TurfgrassCertificates.Count</td>
            </tr>
            }
        </tbody>
    </table>
</div>
