@{
    string name = "FarmCounty";
    var selectListItems = new List<SelectListItem>();
    foreach (var county in Model.Counties)
    {
        selectListItems.Add(new SelectListItem { Text = county.CountyName, Value = county.CountyId.ToString() } );
    }  
    var selectList = new SelectList(selectListItems, "Value", "Text"); 
}
<label>County:</label><em class="required">â€¢</em>
@Html.DropDownList(name, selectList, new { @class="form-control" })
@Html.ValidationMessage("County", "", new { @class = "text-danger" })