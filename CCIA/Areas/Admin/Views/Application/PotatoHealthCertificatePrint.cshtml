                        <tr><td class="box">@Html.Raw(row.MosaicPercent)</td></tr>
                        <tr><td class="box">@Html.Raw(row.OtherVarietyPercent)</td></tr>
                        <tr><td class="box">@Html.Raw(row.BlacklegPercent)</td></tr>
                        <tr><td class="box" style="white-space: nowrap;">@Html.Raw(row.OtherDiseasePercent)</td></tr>
